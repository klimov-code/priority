{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules", "node_modules/**/*.{j,t}s"]
  },
  "rules": {
    "prettier": true,
    "ordered-imports": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  }
}
