{
  "name": "priority",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "rootDir": "src",
  "scripts": {
    "start": "parcel index.html --no-autoinstall --open",
    "prebuild": "rm -rf dist",
    "build": "parcel build index.html --experimental-scope-hoisting",
    "format": "prettier --write",
    "lint": "tslint -p tsconfig.json -c tslint.json -e 'node_modules/**/*'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@tippy.js/react": "^2.1.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-select": "^2.4.3",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "src": "^1.1.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-beautiful-dnd": "^11.0.2",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-select": "^2.0.17",
    "@types/redux-immutable": "^4.0.1",
    "@types/styled-components": "^4.1.14",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.17.0",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5"
  },
  "//": "im in love with parcel bugs",
  "alias": {
    "Actions": "./src/Actions",
    "Components": "./src/Components",
    "Constants": "./src/Constants",
    "Definitions": "./src/Definitions",
    "Reducers": "./src/Reducers",
    "Sagas": "./src/Sagas",
    "Selectors": "./src/Selectors",
    "Store": "./src/Store",
    "Utils": "./src/Utils",
    "Views": "./src/Views"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{ts, tsx}": [
        "yarn lint",
        "git add"
      ]
    },
    "ignore": [
      "node_modules",
      "dist"
    ]
  }
}
